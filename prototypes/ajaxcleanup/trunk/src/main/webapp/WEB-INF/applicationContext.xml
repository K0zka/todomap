<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="messenger" class="org.todomap.prototypes.ajaxcleanup.channel.MessengerImpl">
	</bean>

	<bean id="timeService" class="org.todomap.prototypes.ajaxcleanup.services.timer.TimeService" init-method="init" destroy-method="shutdown">
		<constructor-arg ref="messenger"/>
	</bean>

</beans>
